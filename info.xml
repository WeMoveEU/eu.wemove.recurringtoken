<?xml version="1.0"?>
<extension key="eu.wemove.recurringtoken" type="module">
  <file>recurringtoken</file>
  <name>Recurring contribution mail-merge token</name>
  <description>Exposes last recurring contribution information as a token
  for CiviMail</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Michal Mach</author>
    <email>michal@wemove.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/WeMoveEU/eu.wemove.recurringtoken</url>
    <url desc="Documentation">https://github.com/WeMoveEU/eu.wemove.recurringtoken</url>
    <url desc="Support">https://github.com/WeMoveEU/eu.wemove.recurringtoken</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-12-16</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Recurringtoken</namespace>
  </civix>
</extension>
